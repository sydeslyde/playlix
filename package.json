{
  "name": "playlix",
  "version": "0.2.1",
  "description": "A small web application to export your Spotify playlists",
  "main": "index.html",
  "scripts": {
    "start": "parcel index.html --out-dir ./dist/ --open",
    "build": "parcel build index.html --no-source-maps --out-dir ./dist/ --public-url .",
    "clean": "del /S /Q dist && FOR /D %p IN (\"dist\\*\") DO rmdir /S /Q \"%p\"",
    "prebuild": "npm run clean",
    "predeploy": "npm run build",
    "deploy": "gh-pages -m \"Deploy %npm_package_version% to gh-pages\" -d ./dist/",
    "postversion": "npm run deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sydeslyde/playlix.git"
  },
  "author": "sydeslyde",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/sydeslyde/playlix/issues"
  },
  "homepage": "https://github.com/sydeslyde/playlix",
  "dependencies": {
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "papaparse": "^5.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "spotify-web-api-js": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "gh-pages": "^3.1.0",
    "parcel-bundler": "^1.12.4"
  }
}
